

<%-include('../../layouts/front/header')%>
<%-include('../../layouts/front/navbar')%>
<%-include('../../layouts/front/banner')%>



 <!-- Begin Page Content -->
 <div class="container-fluid">

    <!-- Page Heading -->
    <h1 class="h3 mb-2 text-gray-800">Tables</h1>
    <p class="mb-4">DataTables is a third party plugin that is used to generate the demo table below.
        For more information about DataTables, please visit the <a target="_blank"
            href="https://datatables.net">official DataTables documentation</a>.</p>

    <!-- DataTales Example -->
    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
        </div>
        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                            <thead>
                                <tr>
                                    
                                    <th>Category </th>
                    
                                    <th>Heading</th>
                                    <th>Body</th>
                                    <th>Image</th>
                                    
                                    <th colspan="3">Action</th>
                                    
                                </tr>
                            </thead>
                            <tfoot>
                                <tr>
                                    <th>Category </th>
                    
                                    <th>Heading</th>
                                   
                                    <th>Body</th>
                                    <th>Image</th>
                                  
                                    <th colspan="3">Action</th>
                                </tr>
                            </tfoot>
                        
                            <tbody>
                           <%all_data.map((data)=>{%>

                                <tr>
                                    
                                    <td><%=data.cat_details.category_name%></td> 
                                    <td><%=data.heading%></td> 
                                    <td><%-data.description%></td> 
                                    <td><img src="../front/uploads/<%=data.image %>" width="100px" height="100px"></td>
                                        
                                    <td><a href="/blog/edit-blog/<%=data._id%>" class="btn btn-success">Edit</a></td>
                                    <td><a href="/blog/view-blog/<%=data._id%>" class="btn btn-success">view</a></td>
                                    <td><a href="/blog/delete-blog/<%=data._id%>" class="btn btn-danger">delete</a></td>
                                                                             

                        
                              
                           <%})%>
                        </tr>
          
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
        <!-- /.container-fluid -->

    </div>

    <%-include('../../layouts/front/footer')%>